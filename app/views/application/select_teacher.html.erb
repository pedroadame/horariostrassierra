<div class="alert">
  <%= t("profesores_no_asignados", count: Teacher.wo_account.count) %> <br>
  <%= t("dinos_quien_eres") %>
</div>
<div class="mdl-grid">
  <div class="mdl-cell mdl-cell--4-offset mdl-cell--4-col mdl-cell--middle mdl-card
  mdl-shadow--6dp">
    <div class="mdl-card__title mdl-color--primary mdl-color-text--white">
      <h2 class="mdl-card__title-text"><%= t('profesores') %></h2>
    </div>
    <table class="mdl-data-table mdl-js-data-table full-width links">
      <tbody>
      <tr><td><%= will_paginate @teachers %></td></tr>
      <% @teachers.each do |r| %>
          <tr>
            <td><%= link_to r.name.mb_chars.upcase, assign_teacher_path(id: r.id), method: :post %></td>
          </tr>
      <% end %>
      </tbody>
    </table>


  </div>
</div>