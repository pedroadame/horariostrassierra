<div class="mdl-grid">
  <div class="mdl-cell mdl-cell--4-offset mdl-cell--4-col mdl-cell--middle mdl-card
  mdl-shadow--6dp">
    <div class="mdl-card__title mdl-color--primary mdl-color-text--white">
      <h2 class="mdl-card__title-text" id="import-xml-title"><%= t('importar_xml') %></h2>
    </div>
    <%= form_tag '/import', multipart: true, id: 'import-xml-form' do %>
        <div class="mdl-card__supporting-text">
          <label for="file"><%= t("file") %>: </label><%= file_field_tag 'file', id: 'file-input', accept:
            'text/xml' %>
          <hr>
          <p class="xml-warning"><strong><%= t('advertencia_importar')%></strong></p>
        </div>
        <div class="mdl-card__actions mdl-card--border card-buttons">
          <%= submit_tag t("submit"), class: 'mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect danger-button', id: 'upload', disabled: true, data: {confirm: t("confirmacion")} %>
          <div class="clear"></div>
        </div>
    <% end %>
  </div>
</div>